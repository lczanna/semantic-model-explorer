relationship Sales_Product
	fromColumn: Sales.ProductKey
	toColumn: Product.ProductKey

relationship Sales_Customer
	fromColumn: Sales.CustomerKey
	toColumn: Customer.CustomerKey

relationship Sales_Date
	fromColumn: Sales.OrderDate
	toColumn: Date.Date

relationship Sales_Date_Inactive
	isActive: false
	crossFilteringBehavior: bothDirections
	fromColumn: Sales.OrderDate
	toColumn: Date.Date

relationship Dotted_Table_Rel
	fromColumn: 'Schema.Sales'.ProductKey
	toColumn: 'Schema.Product'.ProductKey
